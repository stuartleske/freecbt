{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "www": "serve www/public",
    "start": "yarn _version && expo start",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "lint:prettier": "prettier -l src/**/*.ts src/**/*.tsx www/public/**/*",
    "format": "prettier --write src/**/*.ts src/**/*.tsx www/public/**/*",
    "test": "yarn lint && yarn lint:prettier && yarn _version && jest",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "//deploy": "don't publish by hand, this is for CI",
    "deploy": "expo login --non-interactive -u $EXPO_USERNAME && yarn _version && expo publish --non-interactive",
    "_version": "./scripts/version.sh > .version.json"
  },
  "dependencies": {
    "@assets/images": "file:assets",
    "@expo/vector-icons": "^10.0.0",
    "dayjs": "^1.8.14",
    "eslint-plugin-react-native-a11y": "^1.2.0",
    "expo": "^37.0.0",
    "expo-analytics-segment": "^8.1.0",
    "expo-constants": "^9.0.0",
    "expo-haptics": "^8.1.0",
    "expo-linking": "^1.0.1",
    "expo-localization": "^8.1.0",
    "expo-splash-screen": "^0.2.3",
    "expokit": "31.1.0",
    "i18n-js": "^3.2.1",
    "jest": "^24.7.1",
    "jest-expo": "^37.0.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.11",
    "makecancelable": "^1.1.1",
    "prop-types": "^15.6.2",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-autogrow-textinput": "^5.2.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-indicators": "^0.13.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-pose": "^0.9.0",
    "react-native-screens": "~2.2.0",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-store-review": "^0.1.5",
    "react-native-svg": "11.0.1",
    "react-native-swiper": "^1.5.14",
    "react-navigation": "^3.0.8",
    "safe-json-stringify": "^1.2.0",
    "sentry-expo": "^2.0.4",
    "tslib": "^1.9.3",
    "typescript": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/expo": "^32.0.10",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-navigation": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^8.1.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "expo-cli": "^3.21.3",
    "prettier": "1.15.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.11",
    "serve": "^11.3.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "prettier": true,
    "space": true,
    "rules": {
      "unicorn/filename-case": false,
      "react/require-default-props": false
    },
    "extends": "xo-react"
  },
  "private": true
}
