{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "www": "serve www/public",
    "start": "yarn _version && expo start",
    "storybook": "yarn _version && EXPO_STORYBOOK=1 expo start --config storybook.config.js",
    "lint": "bash -O globstar -c \"tslint src/**/*.{ts,tsx}\"",
    "lint:prettier": "bash -O globstar -c \"prettier -l src/**/*.{ts,tsx} www/public/**/*.html\"",
    "format": "bash -O globstar -c \"prettier --write src/**/*.{ts,tsx} www/public/**/*.html\"",
    "test": "yarn lint && yarn lint:prettier && yarn _version && jest",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "//deploy": "don't publish by hand, this is for CI",
    "deploy": "yarn _version && expo publish --non-interactive",
    "_version": "./scripts/version.sh > .version.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "dayjs": "^1.8.28",
    "expo": "^37.0.12",
    "expo-analytics-segment": "^8.1.0",
    "expo-constants": "^9.0.0",
    "expo-haptics": "^8.1.0",
    "expo-linking": "^1.0.1",
    "expo-localization": "^8.1.0",
    "expo-splash-screen": "^0.2.3",
    "i18n-js": "^3.7.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-autogrow-textinput": "^5.2.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-pose": "^0.9.0",
    "react-native-screens": "~2.8.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-store-review": "^0.1.5",
    "react-native-swiper": "^1.5.14",
    "react-navigation": "^3.0.8",
    "safe-json-stringify": "^1.2.0",
    "sentry-expo": "^2.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@storybook/addon-ondevice-notes": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/cli": "^5.3.19",
    "@storybook/react-native": "^5.3.19",
    "@types/expo": "^33.0.1",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.62.13",
    "@types/react-navigation": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^8.2.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native-a11y": "^2.0.0",
    "expo-cli": "^3.21.3",
    "jest": "^24.7.1",
    "jest-expo": "^37.0.0",
    "prettier": "2.0.5",
    "react-native-dotenv": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.11",
    "serve": "^11.3.1",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.17.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "prettier": true,
    "space": true,
    "rules": {
      "unicorn/filename-case": false,
      "react/require-default-props": false
    },
    "extends": "xo-react"
  },
  "private": true
}
